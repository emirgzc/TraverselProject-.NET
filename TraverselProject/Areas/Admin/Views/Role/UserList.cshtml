@model List<AppUser>

@{
	ViewData["Title"] = "UserList";
	Layout = "~/Views/Shared/AdminLayout.cshtml";
}


<div class="row">
	<div class="col-lg-12">
		<div class="card">
			<div class="card-body">
				<div class="active-member">
					<div class="table-responsive">
						<table class="table table-xs mb-0">
							<thead>
								<tr>
									<th>Kullanıcı Adı</th>
									<th>Rol Ata</th>
								</tr>
							</thead>
							<tbody>

								@foreach (var item in Model)
								{
									<tr>
										<td>@item.UserName</td>
										<td><a href="/Admin/Role/AssignRole/@item.Id" class="btn btn-warning">Rol Ata</a></td>
									</tr>
								}
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<a href="/Admin/Guide/AddGuide" class="btn btn-success">Yeni Ekle</a>