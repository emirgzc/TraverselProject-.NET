
@model VisitorModel

@{
    ViewData["Title"] = "AddVisitor";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h4>Api İle Ziyaretçi Ekleme</h4>

<form method="post">
	@Html.Label("Ad")
	<input type="text" asp-for="Name" class="form-control" />
	<br />
	@Html.Label("Soyad")
	<input type="text" asp-for="Surname" class="form-control" />
	<br />
	@Html.Label("Şehir")
	<input type="text" asp-for="City" class="form-control" />
	<br />
	@Html.Label("Ülke")
	<input type="text" asp-for="Country" class="form-control" />
	<br />
	@Html.Label("Mail Adresi")
	<input type="text" asp-for="Mail" class="form-control" />
	<br />	
	<button class="btn btn-outline-dark">Ziyaretçi Oluştur</button>
</form>



